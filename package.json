{
  "name": "babel-transform-roll20",
  "version": "0.0.1",
  "description": "A babel transform tool for transpiling scripts for the Roll20 sandbox.",
  "main": "src/index.js",
  "scripts": {
    "lint": "standard",
    "test": "NODE_ENV=test BABEL_DISABLE_CACHE=1 ava"
  },
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@babel/parser": "^7.2.3",
    "@babel/preset-env": "^7.2.3",
    "ava": "^1.0.1",
    "standard": "^12.0.1"
  },
  "peerDependencies": {
    "@babel/core": "^7.2.2"
  },
  "ava": {
    "concurrency": 1,
    "files": [
      "test/**/*.js",
      "!test/fixtures/**/*.js",
      "!test/transforms/**/*.js"
    ]
  },
  "standard": {
    "ignore": [
      "test/fixtures",
      "test/transforms"
    ]
  }
}
